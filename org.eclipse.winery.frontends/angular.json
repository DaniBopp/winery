    "servicecompositionmodeler": {
      "root": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/servicecompositionmodeler",
            "index": "app/servicecompositionmodeler/src/index.html",
            "main": "app/servicecompositionmodeler/src/main.ts",
            "polyfills": "app/servicecompositionmodeler/src/polyfills.ts",
